<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Smoke Suite" parallel="tests" thread-count="1">

    <listeners>
        <listener class-name="com.keanest.framework.reports.TestListener"/>
    </listeners>

    <test name="Smoke Tests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <packages>
            <package name="com.keanest.tests.testcases"/>
        </packages>
    </test>
</suite>
